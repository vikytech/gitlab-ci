{
  "manifest_version": 2,
  "name": "GitLab-CI",
  "description": "This is CI monitor gitlab",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "dashboard.html"
  },
  "background": {
    "pages": "dashboard.html",
    "css": [
      "dashboard.css"
    ],
    "scripts": [
      "jquery.js",
      "dashboard.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "https://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "*.css"
  ]
}